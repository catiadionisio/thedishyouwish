                                                    <% else %>
                                                        <% estado_extra_ea = "inactivo" %>
                                                    <% end %>
                                                    <%= content_tag :div, { :class => ("janela-opcao " + estado_extra_ea), :data => { :extra=> estado_extra_ea}} do %>
                                                        <input type="checkbox" id='entrada-a' class="entrada" value="entrada">
                                                        <label for="entrada-a">Entrada</label>
                                                    <% end %>

                                                    
                                                    <% if session["sobremesaa_ee"] == "activo" %>
                                                        <% estado_extra_sa = "activo" %>
                                                    <% else %>
                                                        <% estado_extra_sa = "inactivo" %>
                                                    <% end %>
                                                    <%= content_tag :div, { :class => ("janela-opcao " + estado_extra_sa), :data => { :extra=> estado_extra_sa}} do %>
                                                        <input type="checkbox" id='sobremesa-a' class="sobremesa" value="sobremesa">
                                                        <label for="sobremesa-a">Sobremesa</label>
                                                    <% end %>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                <% end %>
                                <div class="ementa-refeicao-container">
                                    <% if session["entradaa_ee"] == "activo" %>
                                    <div class="ementa-refeicao-secundaria ementa-entrada" id="2">
                                    <% else %>
                                    <div class="ementa-refeicao-secundaria ementa-entrada" style="display: none;" id="2">
                                    <% end %>
                                        <% i=0 %>
                                        <% @letter_array.each do |letter| %>
                                            <% receita_celula_nro = EmentaReceita.where(:ementa_id => @editarEmenta.id, :celula => (letter+"2")).first %>

                                            <% if ((session[letter+"2_ee"].blank? && receita_celula_nro.blank?) || session[letter+"2_ee_empty"] == 1)  %>
