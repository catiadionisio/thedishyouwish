                                                    <% end %>
                                                    <div class="nome-div">
                                                        <span class="tc nome">
                                                            <%= link_to receita_celula.nome, "receita/#{receita_celula[:id]}", { :target => :blank } %>
                                                        </span>
                                                    </div>
                                                    <div class="opcoes-receita">
                                                        <div class="icon-div">
                                                            <%= link_to("v", "javascript:;", { :class => "glyphter reload", :data => { :celula=> letter+"1", :receitaid => receita_celula.id, :usi => (user_signed_in? ? 1 : 0) } }) %>
                                                        </div>
                                                        <div class="icon-div">
                                                            <%= link_to("N", "javascript:;", { :class => "glyphter delete", :data => { :celula=> letter+"1", :usi => (user_signed_in? ? 1 : 0) } }) %>
                                                        </div>
                                                    </div>
                                                    <div class="ementa-receita-option">
                                                        <a href="javascript:;" class="link-receita-option" ><%= image_tag 'arrow-option.png', :alt => 'editar' %></a>
                                                        <div class="janela-opcoes-container">
                                                            <%= image_tag 'arrow-option-up.png', :alt => '', :class => 'arrow-up' %>
                                                            <div class="janela-opcoes-container2">
                                                                <div class="janela-opcoes-titulo">
                                                                    Editar
                                                                </div>
                                                                <div class="janela-opcoes-grupo">
                                                                    <select class="select-ementa receita-pessoas">
                                                                        <% if !session[letter+"1_ee"].blank? %>
                                                                            <% if !session[letter+"1_ee"][:pessoas].blank? %>
                                                                                <%= options_for_select({ "2 Pessoas" => "2", "3 Pessoas" => "3", "4 Pessoas" => "4", "5 Pessoas" => "5", "6 Pessoas" => "6", "7 Pessoas" => "7", "8 Pessoas" => "8" }, session[letter+"1_ee"][:pessoas].to_s) %>
                                                                            <% else %>
                                                                                <option value="2">2 Pessoas</option>
                                                                                <option value="3">3 Pessoas</option>
                                                                                <option value="4">4 Pessoas</option>
                                                                                <option value="5">5 Pessoas</option>
                                                                                <option value="6">6 Pessoas</option>
                                                                                <option value="7">7 Pessoas</option>
                                                                                <option value="8">8 Pessoas</option>
                                                                            <% end %>
                                                                        <% else %>
                                                                            <%= options_for_select({ "2 Pessoas" => "2", "3 Pessoas" => "3", "4 Pessoas" => "4", "5 Pessoas" => "5", "6 Pessoas" => "6", "7 Pessoas" => "7", "8 Pessoas" => "8" }, receita_celula_nro.npessoas.to_s) %>
                                                                        <% end %>
                                                                    </select>
                                                                    <div class="janela-opcao">
                                                                        <%= image_tag 'lock.png', :alt => 'bloquear' %>
                                                                        <% if !session[letter+"1_ee"].blank? %>
                                                                            <% if !session[letter+"1_ee"][:lock].blank? %>
                                                                                <%= link_to((session[letter+"1_ee"][:lock] == "lock" ? "Desbloquear" : "Bloquear"), "javascript:;", { :class => "bloquear", :data => { :celula=> letter+"1" } }) %>
                                                                            <% else %>
                                                                                <%= link_to("Bloquear", "javascript:;", { :class => "bloquear", :data => { :celula=> letter+"1" } }) %>
                                                                            <% end %>
                                                                        <% else %>
                                                                            <%= link_to("Bloquear", "javascript:;", { :class => "bloquear", :data => { :celula=> letter+"1" } }) %>
                                                                        <% end %>
                                                                    </div>
                                                                </div>
                                                                <div class="janela-opcoes-titulo titulo2">
                                                                    Preencher com
                                                                </div>
                                                                <div class="janela-opcoes-grupo">
                                                                    <% if user_signed_in? %>
                                                                        <div class="janela-opcao">
                                                                            <%= image_tag 'icon-fav.png', :alt => 'fav' %>
                                                                            <%= link_to("Favoritos", "javascript:;", { :class => "add_from_fav", :data => { :celula=> letter+"1" } }) %>
                                                                        </div>
                                                                    <% end %>
                                                                    <div class="janela-opcao">
                                                                        <%= image_tag 'icon-all2.png', :alt => 'todos' %>
                                                                        <%= link_to("Todos", "javascript:;", { :class => "add_from_all", :data => { :celula=> letter+"1" } }) %>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                <% end %>
                                                <% i+=1 %>
                                            <% end %>
                                        <% end %>
                                        <div class="clearfix"></div>
                                    </div>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                            <div class="ementa-refeicao">
                                <%= content_tag :div, { :class => (session["a_lock_ee"] == "lock" ? "ementa-nome lock" : "ementa-nome")} do %>
                                    <div class="ementa-nome-container">
                                        <span class="tc">
                                            <div class="nome">Almoço</div>
                                        </span>
                                    </div>
                                    <div class="opcoes-receita">
                                        <div class="icon-div">
                                            <%= link_to("v", "/ee_add_ementa_refeicao?celula=a", { :class => "glyphter reload" }) %>
                                        </div>
                                        <div class="icon-div">
                                            <%= link_to("N", "/ee_ementa_delete_refeicao?celula=a", { :class => "glyphter delete" }) %>
                                        </div>
                                    </div>
                                    <div class="ementa-receita-option">
                                        <a href="javascript:;" class="link-receita-option" ><%= image_tag 'arrow-option.png', :alt => 'editar' %></a>
                                        <div class="janela-opcoes-container">
                                            <%= image_tag 'arrow-option-up.png', :alt => '', :class => 'arrow-up' %>
                                            <div class="janela-opcoes-container2">
                                                <div class="janela-opcoes-titulo">
                                                    Editar
                                                </div>
                                                <div class="janela-opcoes-grupo" id="a_pessoas">
                                                    <select class="select-ementa refeicao-pessoas">
                                                        <% if !session["a_pessoas_ee"].blank? %>
                                                            <%= options_for_select({ "2 Pessoas" => "2", "3 Pessoas" => "3", "4 Pessoas" => "4", "5 Pessoas" => "5", "6 Pessoas" => "6", "7 Pessoas" => "7", "8 Pessoas" => "8" }, session["a_pessoas_ee"].to_s) %>
                                                        <% else %>
                                                            <%= options_for_select({ "2 Pessoas" => "2", "3 Pessoas" => "3", "4 Pessoas" => "4", "5 Pessoas" => "5", "6 Pessoas" => "6", "7 Pessoas" => "7", "8 Pessoas" => "8" }, @editarEmenta.npessoas.to_s) %>
                                                        <% end %>
                                                    </select>
                                                    <div class="janela-opcao">
                                                        <%= image_tag 'lock.png', :alt => 'bloquear' %>
                                                        <%= link_to((session["a_lock_ee"] == "lock" ? "Desbloquear" : "Bloquear"), "javascript:;", { :class => "bloquear_refeicao", :data => { :celula=> "a_lock" } }) %>
