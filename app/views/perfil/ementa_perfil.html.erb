                                            <span class="ementa-dia-nro"><%= (@date+1).day %></span>
                                            <%= l(@date+1, format: :day_format) %>
                                            <!-- <%=h (@date+1).strftime("%a") %> -->
                                        </div>
                                        <div class="ementa-dia" id="c">
                                            <span class="ementa-dia-nro"><%= (@date+2).day %></span>
                                            <%= l(@date+2, format: :day_format) %>
