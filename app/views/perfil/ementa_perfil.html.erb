                                            <!-- <%=h (@date+4).strftime("%a") %> -->
                                        </div>
                                        <div class="ementa-dia" id="f">
                                            <span class="ementa-dia-nro"><%= (@date+5).day %></span>
                                            <%= l(@date+5, format: :day_format) %>
                                            <!-- <%=h (@date+5).strftime("%a") %> -->
                                        </div>
                                        <div class="ementa-dia" id="g">
                                            <span class="ementa-dia-nro"><%= (@date+6).day %></span>
                                            <%= l(@date+6, format: :day_format) %>
                                            <!-- <%=h (@date+6).strftime("%a") %> -->
                                        </div>
                                    </div>
                                    <%= link_to "f", "/ementa_perfil/#{@editarEmenta.id}/ee_next_day", {:class=>"next-day glyphter"} %>
                                </div>
                            </div>
                            <div class="ementa-refeicao">
                                <%= content_tag :div, { :class => (session["pa_lock_ee"] == "lock" ? "ementa-nome lock" : "ementa-nome")} do %>
