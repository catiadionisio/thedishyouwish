                                        <span class="tc">
                                            <div class="nome">Pequeno-almoço</div>
                                        </span>
                                    </div>
                                    <div class="opcoes-receita">
                                        <div class="icon-div">
                                            <%= link_to("v", "/ee_add_ementa_refeicao?celula=pa", { :class => "glyphter reload" }) %>
                                        </div>
                                        <div class="icon-div">
                                            <%= link_to("N", "/ee_ementa_delete_refeicao?celula=pa", { :class => "glyphter delete" }) %>
                                        </div>
                                    </div>
                                    <div class="ementa-receita-option">
                                        <a href="javascript:;" class="link-receita-option" ><%= image_tag 'arrow-option.png', :alt => 'editar' %></a>
                                        <div class="janela-opcoes-container">
                                            <%= image_tag 'arrow-option-up.png', :alt => '', :class => 'arrow-up' %>
                                            <div class="janela-opcoes-container2">
                                                <div class="janela-opcoes-titulo">
                                                    Editar
                                                </div>
                                                <div class="janela-opcoes-grupo" id="pa_pessoas">
                                                    <select class="select-ementa refeicao-pessoas">
                                                        <% if !session["pa_pessoas_ee"].blank? %>
                                                            <%= options_for_select({ "2 Pessoas" => "2", "3 Pessoas" => "3", "4 Pessoas" => "4", "5 Pessoas" => "5", "6 Pessoas" => "6", "7 Pessoas" => "7", "8 Pessoas" => "8" }, session["pa_pessoas_ee"].to_s) %>
                                                        <% else %>
                                                            <%= options_for_select({ "2 Pessoas" => "2", "3 Pessoas" => "3", "4 Pessoas" => "4", "5 Pessoas" => "5", "6 Pessoas" => "6", "7 Pessoas" => "7", "8 Pessoas" => "8" }, @editarEmenta.npessoas.to_s) %>
                                                        <% end %>
                                                    </select>
                                                    <div class="janela-opcao">
                                                        <%= image_tag 'lock.png', :alt => 'bloquear' %>
                                                        <%= link_to((session["pa_lock_ee"] == "lock" ? "Desbloquear" : "Bloquear"), "javascript:;", { :class => "bloquear_refeicao", :data => { :celula=> "pa_lock" } }) %>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                <% end %>
                                <div class="ementa-refeicao-container">
                                    <div class="ementa-refeicao-principal alone" id="1">
                                        <% @letter_array = ["a","b","c", "d", "e", "f", "g"] %>
                                        <% i=0 %>
                                        <% @letter_array.each do |letter| %>
                                            <% receita_celula_nro = EmentaReceita.where(:ementa_id => @editarEmenta.id, :celula => (letter+"1")).first %>

                                            <% if ((session[letter+"1_ee"].blank? && receita_celula_nro.blank?) || session[letter+"1_ee_empty"] == 1)  %>
                                                <%= content_tag :div, { :class => (letter == "g" ? "ementa-receita empty last" : "ementa-receita empty"), :id => letter+"1"} do %>
                                                    <span class="tc">
                                                        <%= content_tag :span, "X", { :class => "add-receita", :data => { :celula=> letter+"1", :usi => (user_signed_in? ? 1 : 0) }} %>
                                                    </span>
                                                <% end %>
                                                <% i+=1 %>
                                            <% else %>
