<% if user_signed_in? %>
    <div id="receitas_fav" class="tdyw-modal-box alpha60"> 
        <div class="tdyw-modal">
          <div class="tdyw-modal-title">
            Favoritos | Escolher receita
            <div class="close2">
                <%= link_to image_tag("selected.png", :border => 0), "javascript:;", {:class => 'tdyw-close-modal'} %>
            </div>
          </div>
          <div class="tdyw-modal-content">
            <div class="mt5">
                <%= form_tag '/ee_receitas_fav', :id => 'receitas_fav_form' do %>
                    <% if @receitas_fav.length == 0 %>
                        <p>Não tens nenhuma receita nos teus favoritos.</p>
                    <% else %>
                        <div id="chosen-restricoes" class="ingredientes-menu">
                            <%= select_tag("receita", options_from_collection_for_select(@receitas_fav, "id", "nome"), {:class =>'chosen-select4', :include_blank => "Receitas"} ) %>
                        </div>
                        <div class="mt10">
                            <%= submit_tag 'Adicionar', :class => 'submit-restricao', :id => 'receitas_fav_submit' %>
                        </div>
                    <% end %>
                <% end %>
            </div>
          </div>
        </div>
    </div> 
<% end %>

<div id="receitas_todos" class="tdyw-modal-box alpha60"> 
    <div class="tdyw-modal">
      <div class="tdyw-modal-title">
        Todos | Escolher receita
        <div class="close2">
            <%= link_to image_tag("selected.png", :border => 0), "javascript:;", {:class => 'tdyw-close-modal'} %>
        </div>
      </div>
      <div class="tdyw-modal-content">
        <div class="mt5">
            <%= form_tag '/ee_receitas_all' do %>
                <% if @receitas.length == 0 %>
                    <p>Não existem receitas disponíveis.</p>
                <% else %>
                    <div id="chosen-restricoes" class="ingredientes-menu">
                        <%= select_tag("receita", options_from_collection_for_select(@receitas_all, "id", "nome"), {:class =>'chosen-select4', :include_blank => "Receitas"} ) %>
                    </div>
                    <div class="mt10">
                        <%= submit_tag 'Adicionar', :class => 'submit-restricao', :id => 'receitas_all_submit' %>
                    </div>
                <% end %>
            <% end %>
        </div>
      </div>
    </div>
</div> 

<div id="wrap">
    <div class="conteudo-pagina perfil">
        <div class="top-perfil">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="foto-perfil">
                            <% if (current_user.photo.exists?) %>
                                <%= image_tag current_user.photo.url(:medium), :alt => 'Foto', :class => 'img-responsive' %>
                            <% else %>
                                <% if (current_user.provider == "facebook") %>
                                    <%= image_tag current_user.mediumFacebookImage, :alt => 'Foto', :class => 'img-responsive' %>
                                <% else %>
                                    <%= image_tag current_user.photo.url(:medium), :alt => 'Foto', :class => 'img-responsive' %>
                                <% end %>
                            <% end %>
                        </div>
                        <div class="info-perfil">
                            <div class="nome-perfil">
                                <%= link_to current_user.nome, perfil_path %>
                            </div>
                            <div class="email-perfil">
                                <%= current_user.email %>
                            </div>
                            <div class="dados-perfil">
                                <% if (!current_user.sexo.empty?) %>
                                    <div class="dados">                            
                                        <%= image_tag current_user.sexo+'.png', :alt => current_user.sexo %>
                                        <%= current_user.sexo.capitalize %>
                                    </div>
                                <% end %>
                                <% if (!current_user.datanasc.blank?) %> 
                                    <div class="dados ml">   
                                        <%= image_tag 'age.png', :alt => 'idade' %>                     
                                        <%= current_user.age(current_user.datanasc) %> anos
                                    </div>
                                <% end %>
                                <div class="dados ml">
                                    <%= image_tag 'edit.png', :alt => 'editar' %>
                                    <%= link_to "Editar perfil", edit_user_registration_path %>
                                </div>
                            </div>
                            <div class="guardados-perfil">
                                <%= link_to("Receitas favoritas (#{@receitas.length})", receitas_favoritas_path, { :class => "button" }) %>
                                <%= link_to("Ementas guardadas (#{@ementas.length})", ementas_guardadas_path, { :class => "button" }) %>
                                <%= link_to("Restrições (#{@restricoes.length})", restricoes_path, { :class => "button" }) %>
                            </div>
                        </div>
                    </div>
                </div>          
            </div>      
        </div>
        <div class="bottom-perfil">
            <div class="title-perfil-container">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12">
                            <% if session["data_ementa_ee"].blank? %>
                                <% session["data_ementa_ee"] = @editarEmenta.data.to_date %>
                            <% end %>
                            <div class="title-perfil">Ementa de <%= l(@editarEmenta.data.to_date, format: :day_long) %></div>
                        </div>
                    </div>
                </div>
            </div>
            <div id="tipoementa" class="editar_ementa_perfil container mt15">
                <% @date = !session["data_ementa_ee"].blank? ? session["data_ementa_ee"].to_date : @editarEmenta.data.to_date %>

                <div id="alerta_ementa">
                    <% if !session[:erro_restricoes].blank? %>
                        <% session.delete(:erro_restricoes) %>
                        <div class="alert_ementa">
                            Algumas receitas não foram adicionadas devido a restrições!
                        </div>
                    <% end %>
                </div>  

                <div id="alerta_ementa">
                    <% if !session[:return_to].blank? %>
                        <% session.delete(:return_to) %>
                        <div class="alert_ementa ementa_salva">
                            A ementa foi salva com sucesso!
                        </div>
                    <% end %>
                </div>      
                        
                <div class="row mt30 hidden-xs">
                    <div class="col-sm-12 text-left">
                        <%= link_to "Guardar alterações",  ee_guardar_ementa_path(:day => @date, :id => @editarEmenta.id), {:class=>"button"} %>
